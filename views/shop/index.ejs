<%- include('../includes/head.ejs') %>
    <link rel="stylesheet" href="/css/styles.css">



    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>


            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <!-- Indicators -->
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1" style="color: black;"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>
                </ol>

                <!-- Wrapper for slides -->
                <div class="carousel-inner">
                    <div class="item active">
                        <img src="/images/banner-1.jpg" alt="New york" class="w-100">
                        <div class="carousel-item">
                            <div class="carousel-caption">
                                <h2 class="car-title "><span class="text-emph">GET</span> THE LOOK</h2>
                                <p class="car-parag ">Surf our collection to find the best and latest fashion, filled with the best quality.
                                </p>
                                <button class="button-big  car-btn">VISIT STORE</button>
                            </div>
                        </div>
                    </div>
                    <div class="item">
                        <img src="/images/banner-2.jpg" alt="Chicago" style="width:100%;">
                        <div class="carousel-item">
                            <div class="carousel-caption">
                                <h2 class="car-title "><span class="text-emph">SAVE</span> MONEY</h2>
                                <p class="car-parag ">Sales are always on in our store, so save up, and surf our on going sales.
                                </p>
                                <button class="button-big car-btn">VISIT SALES</button>
                            </div>
                        </div>
                    </div>
                    <div class="item">
                        <img src="/images/banner-3.jpg" alt="Chicago" style="width:100%;">
                        <div class="carousel-item">
                            <div class="carousel-caption">
                                <h2 class="car-title "><span class="text-emph">GET</span> THE LOOK</h2>
                                <p class="car-parag ">Surf our collection to find the best and latest fashion, filled with the best quality.
                                </p>
                                <button class="button-big  car-btn">VISIT STORE</button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Left and right controls -->
                <a class="left carousel-control" href="#myCarousel" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="right carousel-control" href="#myCarousel" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>

            <div class="section">
                <h1 class="car-title" style="-webkit-text-stroke: 0ch;"><span class="text-emph" style="-webkit-text-stroke: 0ch;">Featured</span> Products</h1>

                <div class="card-group">
                    <div class="card">
                        <img src="..." class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">Card title</h5>
                            <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>


                            <div class="card-footer">
                                <ul class="reviews">
                                    <% for ( var i=1; i <=5; i++ ){%>
                                        <li>
                                            <i class="far fa-star fa-sm"></i>
                                        </li>
                                        <%}%>
                                </ul>
                                <div class="card-btns">

                                    <button class="button-small card-btn">Add To <i
                                            class="fas fa-shopping-cart"></i></button>
                                    <button class="wishlist "><i class="far fa-heart"></i></button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="card">
                        <img src="..." class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">Card title</h5>
                            <p class="card-text">This card has supporting text below as a natural lead-in to additional content.
                            </p>

                            <div class="card-footer">
                                <ul class="reviews">
                                    <% for ( var i=1; i <=5; i++ ){%>
                                        <li>
                                            <i class="far fa-star fa-sm"></i>
                                        </li>
                                        <%}%>
                                </ul>
                                <div class="card-btns">

                                    <button class="button-small card-btn">Add To <i
                                            class="fas fa-shopping-cart"></i></button>
                                    <button class="wishlist "><i class="far fa-heart "></i></button>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="card">
                        <img src="..." class="card-img-top" alt="...">
                        <div class="card-body">
                            <h5 class="card-title">Card title</h5>
                            <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This card has even longer content than the first to show that equal height action.</p>

                            <div class="card-footer">
                                <ul class="reviews">
                                    <% for ( var i=1; i <=5; i++ ){%>
                                        <li>
                                            <i class="far fa-star fa-sm"></i>
                                        </li>
                                        <%}%>
                                </ul>
                                <div class="card-btns">

                                    <button class="button-small card-btn">Add To <i
                                            class="fas fa-shopping-cart"></i></button>
                                    <button class="wishlist "><i class="far fa-heart"></i></button>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div class="section">
                <h1 class="car-title" style="-webkit-text-stroke: 0ch;"><span class="text-emph" style="-webkit-text-stroke: 0ch;">SHOP</span> Our Categories</h1>
                <div class="card" style="border: none;">
                    <div class="overlayed">
                        <img src="/images/cat-women.jpg" class="card-img-top" alt="...">
                        <div class="overlay">
                            <h5 class="overlay-text">WOMEN</h5>
                        </div>
                    </div>
                </div>
                <div class="card" style="border: none;">
                    <div class="overlayed">
                        <img src="/images/cat-men.jpg" class="card-img-top" alt="...">
                        <div class="overlay">
                            <h5 class="overlay-text">MEN</h5>
                        </div>
                    </div>
                </div>
                <div class="card" style="border: none;">
                    <div class="overlayed">
                        <img src="/images/cat-kids.jpg" class="card-img-top" alt="...">
                        <div class="overlay">
                            <h5 class="overlay-text">KIDS</h5>
                        </div>
                    </div>
                </div>
                <div class="card" style="border: none;">
                    <div class="overlayed">
                        <img src="/images/cat-acces.jpg" class="card-img-top" alt="...">
                        <div class="overlay">
                            <h5 class="overlay-text" style="left: 15%; font-size: 40px">ACCESSORIES</h5>
                        </div>
                    </div>
                </div>


            </div>

            <%- include('../includes/footer.ejs') %>

